<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Biomes</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="AB_ExoticDoor" or defName="AB_ExoticWall" or defName="AB_ExoticColumn"]</xpath>
					<value>
						<researchPrerequisites>
							<li>VFET_Construction</li>
						</researchPrerequisites>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/TerrainDef[@Name="AB_FloorBase" or @Name="AB_AlienWoodFloorsBase"]</xpath>
					<value>
						<researchPrerequisites>
							<li>VFET_Construction</li>
						</researchPrerequisites>
					</value>
				</li>
				
			</operations>
		</match>
	</Operation>
</Patch>

