<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Apparel Expanded</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="VAE_Apparel_PeltCoat" or defName="VAE_Apparel_TribalPoncho" or defName="VAE_Apparel_TribalKilt"]/recipeMaker</xpath>
					<value>
						<researchPrerequisite>VFET_Tribalwear</researchPrerequisite>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>

