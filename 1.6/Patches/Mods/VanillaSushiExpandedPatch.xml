<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Cooking Expanded - Sushi</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="VCE_Soybean" or defName="VCE_Wasabi"]/plant</xpath>
					<value>
						<sowResearchPrerequisites>
							<li>VFET_Cultivation</li>
						</sowResearchPrerequisites>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="VCE_SushiPrepTable"]</xpath>
					<value>
						<researchPrerequisites>
							<li>VFET_Culture</li>
						</researchPrerequisites>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>

