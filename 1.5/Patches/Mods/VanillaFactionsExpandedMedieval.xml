<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			 <li>Vanilla Factions Expanded - Medieval 2</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
			
				<li Class="PatchOperationAdd">
					<xpath>Defs/TerrainDef[defName="VFEM2_Chemdrench"]</xpath>
					<value>
						<researchPrerequisites>
							<li>VFET_Construction</li>
						</researchPrerequisites>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/RecipeDef[defName="VFEM2_Make_Hardweave"]</xpath>
					<value>
						<researchPrerequisite>VFET_Agriculture</researchPrerequisite>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/RecipeDef[defName="VFEM2_Make_Hayweave"]</xpath>
					<value>
						<researchPrerequisite>VFET_Agriculture</researchPrerequisite>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="VFEM2_Palisade"]</xpath>
					<value>
						<researchPrerequisites>
							<li>VFET_Construction</li>
						</researchPrerequisites>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="VFEM2_Apparel_TorchBelt"]</xpath>
					<value>
						<researchPrerequisites>
							<li>VFET_Fire</li>
						</researchPrerequisites>
					</value>
				</li>
	
			</operations>
		</match>
	</Operation>
</Patch>

