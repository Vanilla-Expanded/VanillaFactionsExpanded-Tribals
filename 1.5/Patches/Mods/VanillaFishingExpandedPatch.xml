<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Fishing Expanded</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				
				<li Class="PatchOperationAdd">
					<xpath>Defs/VFETribals.TribalResearchProjectDef[defName="VFET_AnimalHandling"]/unlocksDesignators</xpath>
					<value>
						<li>VCE_Fishing.Designator_ZoneAdd_Fishing</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/VFETribals.TribalResearchProjectDef[defName="VFET_AnimalHandling"]/unlocksWorkTypes</xpath>
					<value>
						<li>VCEF_Fishing</li>
					</value>
				</li>
				
				
			</operations>
		</match>
	</Operation>
</Patch>

