<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Factions Expanded - Vikings</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="VFEV_MooseHeaddress" or defName="VFEV_BearHeaddress"]/recipeMaker</xpath>
					<value>
						<researchPrerequisite>VFET_Tribalwear</researchPrerequisite>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="VFEV_Apparel_TorchBelt"]/recipeMaker</xpath>
					<value>
						<researchPrerequisite>VFET_Weapons</researchPrerequisite>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>

